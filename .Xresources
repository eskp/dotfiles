!*.buffered:     true
*.cursorColor:  green
*.scrollBar:    false
*.font:         -*-terminus-medium-*-*-*-14-*-*-*-*-*-*-*
! open URLs in a browser
URxvt.perl-ext-common:  default,url-select
URxvt.keysym.M-u:       perl:url-select:select_next
URxvt.urlLauncher:      /usr/bin/chromium
URxvt.underlineURLs:    true
